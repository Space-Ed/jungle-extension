{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "noImplicitUseStrict": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "compileOnSave": true,
    "buildOnSave": false,
    "exclude": [
        "node_modules"
    ],
    "atom": {
        "rewriteTsconfig": true
    },
    "filesGlob": [
        "./node_modules/jungle-core/ts_src/jungle.ts",
        "./index.ts"
    ],
    "files": [
        "./node_modules/jungle-core/ts_src/jungle.ts",
        "./index.ts"
    ]
}
